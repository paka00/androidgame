<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tausta_achievements"
    tools:context=".JoggingFragment">

    <TextView
        android:id="@+id/tv1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:fontFamily="@font/pixel_font"
        android:layout_marginTop="60dp"
        android:text="Juu"/>

    <TextView
        android:id="@+id/tv2"
        android:layout_width="match_parent"
        android:fontFamily="@font/pixel_font"
        android:layout_height="50dp"
        android:text="jees"
        android:gravity="center"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_gravity="center"
        android:id="@+id/start_jog_button"
        android:background="@drawable/start_run_button"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/pixel_font"
        android:id="@+id/prev_walk_stats"
        android:gravity="center"/>



</FrameLayout>